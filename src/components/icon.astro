---

import { Image } from "astro:assets";
import Profile from '../../public/images/profile.png'

const {
    height = 150,
    width = 150,
} = Astro.props

---

<Image 
    src={ Profile }
    alt="Cargando imagen..." 
    width={ width }
    height={ height }
    draggable="false"
    class="rounded-md shadow-lg"
/>